{
  "name": "tarot-garden",
  "version": "1.0.0",
  "description": "A beautiful mobile tarot card app built with Vue and Capacitor",
  "author": "Tarot Garden Team",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "copy:assets": "copy-assets.bat",
    "build": "vite build",
    "build:cap": "vite build --config vite.config.capacitor.ts",
    "preview": "vite preview",
    "vercel-build": "vite build",
    "build:android": "npm run build:cap && npx cap sync android --config capacitor.config.android.ts",
    "cap:init": "cap init",
    "cap:add:android": "cap add android",
    "cap:add:ios": "cap add ios",
    "cap:sync": "npm run build && cap sync",
    "cap:run:android": "cap run android",
    "cap:run:ios": "cap run ios",
    "cap:open:android": "cap open android",
    "cap:open:ios": "cap open ios",
    "cap:build:android": "cap build android",
    "cap:build:ios": "cap build ios"
  },
  "dependencies": {
    "@capacitor-community/admob": "^6.2.0",
    "@capacitor/app": "^6.0.2",
    "@capacitor/browser": "^6.0.5",
    "@capacitor/core": "^6.2.1",
    "@capacitor/haptics": "^6.0.2",
    "@capacitor/keyboard": "^6.0.3",
    "@capacitor/share": "^6.0.3",
    "@capacitor/status-bar": "^6.0.2",
    "@capacitor/toast": "^6.0.3",
    "@revenuecat/purchases-capacitor": "^8.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "pinia": "^2.1.7",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@capacitor/android": "^6.2.1",
    "@capacitor/cli": "^6.2.1",
    "@capacitor/ios": "^6.2.1",
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "fs-extra": "^11.3.0",
    "sass": "^1.69.0",
    "sharp": "^0.34.3",
    "terser": "^5.43.1",
    "typescript": "~5.3.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.0"
  }
}
